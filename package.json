    "@types/ldapjs": "^3.0.6",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "sirv": "^3.0.2",
    "tailwindcss": "^4.1.18",
    "typedoc": "^0.27.0",
    "typescript": "~5.8.2",
    "vite": "^7.3.1"
  }
